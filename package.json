{
  "name": "mi-app-responsiva",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://javiii3er.github.io/mi-app-responsiva",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --fix",
